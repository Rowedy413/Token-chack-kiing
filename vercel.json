{
  "version": 2,
  "builds": [
    {
      "src": "api/validate.py",
      "use": "@vercel/python"
    },
    {
      "src": "public/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/api/validate", "dest": "api/validate.py" },
    { "src": "/(.*)", "dest": "/public/index.html" }
  ]
}
